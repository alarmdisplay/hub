{
  "name": "@alarmdisplay/hub",
  "description": "Backend for collecting, processing and forwarding alerts",
  "version": "1.0.0-beta.4",
  "homepage": "https://alarmdisplay.org",
  "private": true,
  "main": "src",
  "author": {
    "name": "Andreas Brain",
    "email": "dev@abrain.de"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.0.0"
  },
  "scripts": {
    "test": "npm run compile && npm run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts",
    "lint:fix": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --no-notify --rs src/",
    "start": "npm run compile && node lib/",
    "jest": "jest --runInBand --forceExit --coverage",
    "jest:watch": "jest --runInBand --forceExit --watch",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "4.5.16",
    "@feathersjs/authentication-local": "4.5.16",
    "@feathersjs/authentication-oauth": "4.5.16",
    "@feathersjs/configuration": "4.5.16",
    "@feathersjs/errors": "4.5.16",
    "@feathersjs/express": "4.5.16",
    "@feathersjs/feathers": "4.5.16",
    "@feathersjs/socketio": "4.5.16",
    "@feathersjs/transport-commons": "4.5.16",
    "axios": "0.27.2",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "feathers-hooks-common": "5.0.6",
    "feathers-sequelize": "6.4.0",
    "feathers-shallow-populate": "2.5.1",
    "gauss-krueger": "1.0.1",
    "joi": "17.8.3",
    "log4js": "6.8.0",
    "mysql2": "2.3.3",
    "sequelize": "6.29.0",
    "serialport": "9.2.8",
    "serve-favicon": "2.5.0",
    "umzug": "2.3.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@types/bcryptjs": "2.4.2",
    "@types/bluebird": "3.5.38",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/jest": "29.4.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/serialport": "8.0.2",
    "@types/serve-favicon": "2.5.3",
    "@types/umzug": "2.3.3",
    "@types/uuid": "8.3.4",
    "@types/validator": "13.7.13",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "feathers-factory": "4.0.2",
    "jest": "29.4.3",
    "nodemon": "2.0.21",
    "shx": "0.3.4",
    "sqlite3": "5.1.4",
    "ts-jest": "29.0.5",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5"
  }
}
