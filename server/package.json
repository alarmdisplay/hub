{
  "name": "@alarmdisplay/hub",
  "description": "Backend for collecting, processing and forwarding alerts",
  "version": "1.0.0-beta.4",
  "homepage": "https://alarmdisplay.org",
  "private": true,
  "main": "src",
  "author": {
    "name": "Andreas Brain",
    "email": "dev@abrain.de"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "test": "npm run compile && npm run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts",
    "lint:fix": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --no-notify --rs src/",
    "start": "npm run compile && node lib/",
    "jest": "jest --runInBand --forceExit --coverage",
    "jest:watch": "jest --runInBand --forceExit --watch",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.15",
    "@feathersjs/authentication-local": "^4.5.15",
    "@feathersjs/authentication-oauth": "^4.5.15",
    "@feathersjs/configuration": "^4.5.15",
    "@feathersjs/errors": "^4.5.15",
    "@feathersjs/express": "^4.5.15",
    "@feathersjs/feathers": "^4.5.15",
    "@feathersjs/socketio": "^4.5.15",
    "@feathersjs/transport-commons": "^4.5.15",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-hooks-common": "^5.0.6",
    "feathers-sequelize": "^6.3.4",
    "feathers-shallow-populate": "^2.5.1",
    "gauss-krueger": "^1.0.1",
    "helmet": "^4.6.0",
    "joi": "^17.6.0",
    "log4js": "^6.6.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.22.1",
    "serialport": "^9.2.8",
    "serve-favicon": "^2.5.0",
    "umzug": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.36",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/serialport": "^8.0.2",
    "@types/serve-favicon": "^2.5.3",
    "@types/umzug": "^2.3.3",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "feathers-factory": "^4.0.2",
    "jest": "^27.5.1",
    "nodemon": "^2.0.20",
    "shx": "^0.3.4",
    "sqlite3": "npm:@vscode/sqlite3@^5.0.8",
    "ts-jest": "^27.1.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  }
}
